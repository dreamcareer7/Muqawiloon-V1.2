$('.gallery-page-product-list').append("<%= j render(:partial => 'gallery/list') %>");
<% if @gallery_items.next_page.present? %>
  $(".pagination-load-more a").attr("href", "<%= gallery_index_path(page: @gallery_items.next_page, category_type: params[:category_type])%>");
<% else %>
  $(".pagination-load-more").hide()
<% end %>
$('.custom_carousel').not('.slick-initialized').slick({
  dots:true,
  centerMode: false,
  rtl: $("html").hasClass("rtl")
});
